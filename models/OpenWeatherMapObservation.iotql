--
--  compile to JSON with 'iotql-model OpenWeatherMapObservation.iotql'
--
CREATE MODEL OpenWeatherMapObservation WITH
    schema:name = "OpenWeatherMapObservation",
    schema:description = "OpenWeatherMapObservation"
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean,
    iot:write = true,
    iot:read = true,
    iot:actuator = true,
    iot:sensor = true
;
